  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/loongegevens>;
  rdfs:range schemas:QuantitativeValue .

<https://data.vlaanderen.be/ns/loongegevens#arbeidsloon> a owl:ObjectProperty;
  rdfs:label "arbeidsloon"@nl;
  rdfs:comment "TO DO"@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/loongegevens#Arbeidsrelatie>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/loongegevens>;
  rdfs:range <https://data.vlaanderen.be/ns/loongegevens#Arbeidsloon> .

<https://data.vlaanderen.be/ns/loongegevens#arbeidsrelatie> a owl:ObjectProperty;
  rdfs:label "arbeidsrelatie"@nl;
  rdfs:comment "TO DO"@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/loongegevens#Arbeidsloon>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/loongegevens>;
