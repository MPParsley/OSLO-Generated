@prefix dc: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix shacl: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl> <https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nlshapes> <https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl#DatasetCatalogusShape>,
    <https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl#LicentieDocumentShape>,
    <https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl#DistributieShape>,
    <https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl#AgentShape>,
    <https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl#DatasetShape> .

<https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl#AgentShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype rdf:langString;
    shacl:definition "De naam van de uitgever"@nl;
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "naam"@nl;
    shacl:path foaf:name
  ];
  shacl:targetClass foaf:Agent .

<https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl#DatasetCatalogusShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class foaf:Agent;
    shacl:definition "De uitgever van de datasetcatalogus"@nl;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "heeft uitgever"@nl;
    shacl:path dc:publisher
  ], [
    shacl:datatype rdf:langString;
    shacl:definition "De naam van de datasetcatalogus."@nl;
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "titel"@nl;
    shacl:path dc:title
  ], [
    shacl:class dc:LicenseDocument;
    shacl:definition "De licentie van de datasetcatalogus."@nl;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "heeft licentie"@nl;
    shacl:path dc:license
  ], [
    shacl:datatype rdf:langString;
    shacl:definition "Een bondige omschrijving van de datasetcatalogus. "@nl;
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "beschrijving"@nl;
    shacl:path dc:description
  ], [
    shacl:class dcat:Dataset;
    shacl:definition "Een dataset die behoort tot de datasetcatalogus"@nl;
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "heeft dataset"@nl;
    shacl:path dcat:dataset
  ];
  shacl:targetClass dcat:Catalog .

<https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl#DatasetShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    <http://purl.org/linked-data/cube#codeList> <https://vocab.belgif.be/auth/datatheme>;
    shacl:class skos:Concept;
    shacl:definition "Het thema waartoe de dataset behoort."@nl;
    shacl:maxCount "n"^^xsd:integer;
    shacl:name "thema"@nl;
    shacl:path dcat:theme
  ], [
    shacl:class vcard:Kind;
    shacl:definition "De contactinformatie via men in contact kan treden met de verantwoordelijken van de dataset. "@nl;
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "contactinfo"@nl;
    shacl:path dcat:contactPoint
  ], [
    shacl:datatype rdf:langString;
    shacl:definition "Een bondige omschrijving van de dataset."@nl;
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "beschrijving"@nl;
    shacl:path dc:description
  ], [
    shacl:datatype rdf:langString;
    shacl:definition "De naam van de dataset"@nl;
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "titel"@nl;
    shacl:path dc:title
  ], [
    shacl:class dcat:Distribution;
    shacl:definition "De distributie van een dataset."@nl;
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "heeft distributie"@nl;
    shacl:path dcat:distribution
  ], [
    shacl:class foaf:Agent;
    shacl:definition "De uitgever van de dataset. "@nl;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "heeft uitgever"@nl;
    shacl:path dc:publisher
  ], [
    <http://purl.org/linked-data/cube#codeList> <https://publications.europa.eu/en/web/eu-vocabularies/at-dataset/-/resource/dataset/access-right>;
    shacl:class skos:Concept;
    shacl:definition "De toegankelijkheidscategorie van de dataset."@nl;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "toegankelijkheid"@nl;
    shacl:path dc:accessRights
  ];
  shacl:targetClass dcat:Dataset .

<https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl#DistributieShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype xsd:anyURI;
    shacl:definition "De URL waar de data kan gevonden worden."@nl;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "toegangsURL"@nl;
    shacl:path dcat:accessURL
  ], [
    shacl:datatype rdf:langString;
    shacl:definition "Een naam voor de distributie"@nl;
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "titel"@nl;
    shacl:path dc:title
  ], [
    shacl:class dc:LicenseDocument;
    shacl:definition "De licentie van de distributie"@nl;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "heeft licentie"@nl;
    shacl:path dc:license
  ];
  shacl:targetClass dcat:Distribution .

<https://data.dev-vlaanderen.be/shacl/DCAT-AP-VL-SHACL_nl#LicentieDocumentShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:targetClass dc:LicenseDocument .

 [
    shacl:datatype xsd:anyURI;
    shacl:definition "Het e-mail adres waarmee je contact kan opnemen"@nl;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "e-mail"@nl;
    shacl:path vcard:hasEmail
  ] .
