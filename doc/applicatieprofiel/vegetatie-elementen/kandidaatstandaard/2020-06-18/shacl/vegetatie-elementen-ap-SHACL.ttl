    shacl:class skos:Concept;
    shacl:definition "De conditie beoordeeld volgens de kronenstructuur van Dr. A. Roloff, gelet op de scheutlengte ontwikkeling en vorming van dood hout."@nl;
    shacl:maxCount 1;
    shacl:name "conditiebeoordeling"@nl;
    shacl:path <https://data.vlaanderen.be/ns/openbaardomein/vegetatie#conditiebeoordeling>
