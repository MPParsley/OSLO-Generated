    {
      "@id": "https://data.vlaanderen.be/ns/financiele-data#NumeriekDatatype.decimalen",
      "@type": "http://www.w3.org/2002/07/owl#DatatypeProperty",
      "description": {
        "nl": "Geeft aan tot hoeveel cijfers na de komma het getal correct is."
      },
