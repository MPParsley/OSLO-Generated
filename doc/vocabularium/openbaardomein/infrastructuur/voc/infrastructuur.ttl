    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/openbaardomein/infrastructuur> ;
    rdfs:subClassOf odi:Weginrichtingselement .

odi:Wegsignalisatie a owl:Class ;
    rdfs:label "Wegsignalisatie"@nl ;
    rdfs:comment "Alle elementen aangebracht ten behoeve van de weggebruiker, bijvoorbeeld: reflectorpaaltjes, palen voor verkeersborden, wegwijzers, plaatsnaamborden, straatnaambord, bord bebouwde kom, signalisatiepalen openbaar vervoer."@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/openbaardomein/infrastructuur> ;
    rdfs:subClassOf odi:Signalisatie .

odi:Sensoropstelling a owl:Class ;
    rdfs:label "Sensoropstelling"@nl ;
    vann:usageNote "De sensor zelf hoeft niet noodzakelijk aanwezig te zijn, bijvoorbeeld een flitspaal zonder een flitscamera. Naar GRB-skeletoptie water- en rioolbeheer - WRM3: Debietmeter; GRB Aanvullende steekkaarten - WPI28: Meteopaal; GRB-skeletoptie wegbeheer - WDL: Detectielus (afgeleid)."@nl ;
    rdfs:comment "De gehele opstelling van een meettoestel, bedoeld om iets waar te nemen."@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/openbaardomein/infrastructuur> ;
    rdfs:subClassOf odi:Infrastructuurelement .

odi:Straatmeubilair a owl:Class ;
    rdfs:label "Straatmeubilair"@nl ;
    rdfs:comment "Straatmeubilair is  een diverse groep van objecten die in het algemeen in de openbare ruimte staan (zowel corridor- als surplusgebied)"@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/openbaardomein/infrastructuur> ;
    rdfs:subClassOf odi:Infrastructuurelement .

odi:Infrastructuurelement a owl:Class ;
    rdfs:label "Infrastructuurelement"@nl ;
    rdfs:comment "Individueel object op het Openbaar Domein dat niet vegetatief is "@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/openbaardomein/infrastructuur> ;
    rdfs:subClassOf <https://data.vlaanderen.be/ns/openbaardomein#Element> .

