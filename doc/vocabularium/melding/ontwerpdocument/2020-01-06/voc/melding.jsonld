  "classes": [
    {
      "@id": "https://data.vlaanderen.be/ns/melding#Applicatie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een artificiële agent in een softwareomgeving."
      },
      "parents": [
