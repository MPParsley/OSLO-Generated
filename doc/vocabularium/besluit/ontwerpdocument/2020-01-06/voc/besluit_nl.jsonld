  "classes": [
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#AanvullendReglement",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een aanvullend reglement op de politie van het wegverkeer is een besluit met betrekking tot de aanpassing van de algemene wegcode aan plaatselijke omstandigheden met een blijvend of periodiek karakter."
      },
      "parents": [
        "http://data.vlaanderen.be/ns/besluit#Besluit"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Agendapunt",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Formeel aanvaard te behandelen onderwerp op een zitting."
      },
      "parents": []
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Artikel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Formeel afgebakend onderdeel van een besluit, dat een of meer van de beoogde rechtsgevolgen beschrijft."
      },
      "parents": [
        "http://purl.org/vocab/cpsv#FormalFramework",
        "http://data.europa.eu/eli/ontology#LegalExpression"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#BehandelingVanAgendapunt",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "De uitvoering van de acties om een agendapunt te verwerken tijdens een zitting."
      },
      "parents": [
        "http://data.vlaanderen.be/ns/besluit#Vergaderactiviteit"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Besluit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "De authentieke schriftelijke neerslag van een beslissing van een bestuursorgaan. Deze beslissing houdt een rechtshandeling in waarbij sprake is van een beoogd rechtsgevolg."
      },
      "usage": {
        "nl": "Hieronder vallen zowel individuele besluiten (rechtshandelingen met een individueel karakter zoals het toekennen van een subsidie) als besluiten van algemene strekking (beslissingen van algemene reglementaire strekking die niet gericht zijn op een individu maar op een groep, zoals de verkeersregels)."
      },
      "parents": [
        "http://xmlns.com/foaf/0.1/Document",
        "http://data.europa.eu/eli/ontology#LegalExpression",
        "http://purl.org/vocab/cpsv#FormalFramework"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Bestuurseenheid",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Publieke, bestuurlijke organisatie verbonden aan een territoriaal omschreven gebied met bepaalde verantwoordelijkheden waarbinnen het bestuurshandelingen kan stellen."
      },
      "parents": [
        "http://data.europa.eu/m8g/PublicOrganisation"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Bestuursorgaan",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een formeel orgaan van een rechtspersoon dat bevoegd is om rechtshandelingen te stellen."
      },
      "parents": [
        "http://www.w3.org/ns/org#Organization"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Innamevergunning",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "De vergunning vor het tijdelijk in gebruik nemen van het openbaar domein."
      },
      "parents": [
        "http://data.vlaanderen.be/ns/besluit#Vergunning"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Signalisatievergunning",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een vergunning op het tijdelijk aanbrengen of wijzigen van signalisatie op het openbaar domein of privaat domein met openbaar karakter."
      },
      "parents": [
        "http://data.vlaanderen.be/ns/besluit#Vergunning"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Stemming",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Geformaliseerde handeling waarbij de stemgerechtigde leden van een bestuursorgaan hun keuze te kennen geven."
      },
      "parents": [
        "http://data.vlaanderen.be/ns/besluit#Vergaderactiviteit"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#TijdelijkReglement",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een aanvullend reglement op de politie van het wegverkeer met een tijdelijke geldigheid."
      },
      "parents": [
        "http://data.vlaanderen.be/ns/besluit#Besluit"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Vergaderactiviteit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een vergadering of fase ervan."
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Vergunning",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een toelating om in een bepaalde tijdspanne een bepaalde activiteit uit te voeren die anders verboden is."
      },
      "parents": [
        "http://data.vlaanderen.be/ns/besluit#Besluit"
      ]
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Voorwaarde",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Datgene waaraan moet voldaan worden om een geldig besluit te zijn."
      },
      "parents": []
    },
    {
      "@id": "http://data.vlaanderen.be/ns/besluit#Zitting",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een geformaliseerde samenkomst van de leden van een bestuursorgaan met het doel om de aangelegenheden te regelen waarvoor het bevoegd is."
      },
      "parents": [
        "http://data.vlaanderen.be/ns/besluit#Vergaderactiviteit"
      ]
    }
  ],
